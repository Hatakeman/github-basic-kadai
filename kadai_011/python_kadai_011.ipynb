      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# for文で繰り返し処理を行う\n",
        "for planet in array:\n",
        "    print(planet)\n",
        "\n",
        "# while文で繰り返し処理を行う\n",
        "i = 0 #indwxで0から呼び出す\n",
        "while i < len(array): #len関数でarrayの数を算出\n",
        "    print(array[i]) #その時のiの惑星名を出力\n",
        "    i += 1 #i=0から出力し、出力後にiを+1していきlen関数で算出したarrayの数に達したらループが終了する"
      ]
